<style>
body {
    font-family: Arial, sans-serif;
    background-color: #f4f4f4;
    margin: 0;
    padding: 20px;
}

h1 {
    text-align: center;
    color: #333;
}

h2 {
    color: #555;
    text-align: center;
    margin-bottom: 20px;
}

div {
    background-color: #fff;
    padding: 20px;
    border-radius: 8px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    margin-bottom: 20px;
    max-width: 400px;
    margin-left: auto;
    margin-right: auto;
}

ul {
    list-style-type: none;
    padding: 0;
}

li {
    margin: 10px 0;
}

a {
    text-decoration: none;
    color: rgb(59 130 246);
    font-weight: bold;
    display: block;
    padding: 10px;
    border-radius: 4px;
    background-color: #f4f4f4;
    text-align: center;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
    transition: background-color 0.3s, color 0.3s;
}

a:hover {
    background-color: rgb(59 130 246);
    color: white;
}
</style>

<div>
    <h1>{{ project_name }} prototype</h1>
    <h2>Welcome, please select an application:</h2>

    <ul>
        {% for user_type in user_types %}
        <li>
            <a href="{% raw %}{%{% endraw %} url '{{ user_type }}-homerender' {% raw %}%}{% endraw %}">{{ user_type }}</a>
        </li>
        {% endfor %}
    </ul>
</div>
