{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/Edge",
    "definitions": {
        "Edge": {
            "anyOf": [
                {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "association"
                        },
                        "multiplicity": {
                            "type": "object",
                            "properties": {
                                "from": {
                                    "$ref": "#/definitions/Kernel.MultiplicityElement"
                                },
                                "to": {
                                    "$ref": "#/definitions/Kernel.MultiplicityElement"
                                }
                            },
                            "additionalProperties": false
                        },
                        "labels": {
                            "type": "object",
                            "properties": {
                                "from": {
                                    "type": "string"
                                },
                                "to": {
                                    "type": "string"
                                }
                            },
                            "additionalProperties": false
                        },
                        "diagram": {
                            "type": "string",
                            "const": "class"
                        },
                        "id": {
                            "type": "string"
                        },
                        "diagramId": {
                            "type": "string"
                        },
                        "isDirected": {
                            "type": "boolean",
                            "const": true
                        },
                        "from": {
                            "type": "string"
                        },
                        "to": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "diagram",
                        "from",
                        "id",
                        "isDirected",
                        "to",
                        "type"
                    ]
                },
                {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "composition"
                        },
                        "diagram": {
                            "type": "string",
                            "const": "class"
                        },
                        "id": {
                            "type": "string"
                        },
                        "diagramId": {
                            "type": "string"
                        },
                        "isDirected": {
                            "type": "boolean",
                            "const": true
                        },
                        "from": {
                            "type": "string"
                        },
                        "to": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "diagram",
                        "from",
                        "id",
                        "isDirected",
                        "to",
                        "type"
                    ]
                },
                {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "generalization"
                        },
                        "multiplicity": {
                            "type": "object",
                            "properties": {
                                "from": {
                                    "$ref": "#/definitions/Kernel.MultiplicityElement"
                                },
                                "to": {
                                    "$ref": "#/definitions/Kernel.MultiplicityElement"
                                }
                            },
                            "additionalProperties": false
                        },
                        "labels": {
                            "type": "object",
                            "properties": {
                                "from": {
                                    "type": "string"
                                },
                                "to": {
                                    "type": "string"
                                }
                            },
                            "additionalProperties": false
                        },
                        "diagram": {
                            "type": "string",
                            "const": "class"
                        },
                        "id": {
                            "type": "string"
                        },
                        "diagramId": {
                            "type": "string"
                        },
                        "isDirected": {
                            "type": "boolean",
                            "const": true
                        },
                        "from": {
                            "type": "string"
                        },
                        "to": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "diagram",
                        "from",
                        "id",
                        "isDirected",
                        "to",
                        "type"
                    ]
                },
                {
                    "type": "object",
                    "properties": {
                        "type": { "const": "dependency" },
                        "label": {
                            "type": "string"
                        }
                    },
                    "required": ["type"]
                },
                {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "isDirected": {
                            "type": "boolean",
                            "const": true
                        },
                        "guard": {
                            "type": "string"
                        },
                        "weight": {
                            "type": "string"
                        },
                        "diagram": {
                            "type": "string",
                            "const": "activity"
                        },
                        "id": {
                            "type": "string"
                        },
                        "diagramId": {
                            "type": "string"
                        },
                        "from": {
                            "type": "string"
                        },
                        "to": {
                            "type": "string"
                        }
                    },
                    "required": ["diagram", "from", "id", "isDirected", "to"]
                },
                {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "type": {
                            "type": "string",
                            "enum": ["interaction", "extension", "inclusion"]
                        },
                        "diagram": {
                            "type": "string",
                            "const": "usecase"
                        },
                        "id": {
                            "type": "string"
                        },
                        "diagramId": {
                            "type": "string"
                        },
                        "isDirected": {
                            "type": "boolean",
                            "const": true
                        },
                        "from": {
                            "type": "string"
                        },
                        "to": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "diagram",
                        "from",
                        "id",
                        "isDirected",
                        "to",
                        "type"
                    ]
                }
            ],
            "description": "Base edge with references for all diagram edges",
            "$id": "https://schema.nguml.com/edge.schema.json"
        },
        "Kernel.MultiplicityElement": {
            "$ref": "#/definitions/MultiplicityElement"
        },
        "MultiplicityElement": {
            "type": "object",
            "properties": {
                "isOrdered": {
                    "type": "boolean"
                },
                "isUnique": {
                    "type": "boolean"
                },
                "lower": {
                    "type": "number"
                },
                "upper": {
                    "anyOf": [
                        {
                            "type": "string",
                            "const": "*"
                        },
                        {
                            "type": "number"
                        }
                    ]
                }
            },
            "additionalProperties": false,
            "$id": "https://schema.nguml.com/kernel/multiplicityelement.schema.json"
        }
    }
}
